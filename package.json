{
  "main": "index.js",
  "scripts": {
    "debug": "ndb nodemon --use_strict index.js",
    "benchmark": "node --use_strict benchmark.js",
    "start": "nodemon --use_strict index.js | pino-pretty",
    "setup": "rm -rf .git && npm install",
    "test": "tap --node-arg=--use_strict --reporter=spec"
  },
  "dependencies": {
    "pino": "^6.13.3",
    "pino-pretty": "^7.0.1"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "eslint": "^8.0.0",
    "ndb": "^1.1.5",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "tap": "^15.0.10"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {}
  }
}
